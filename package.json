{
  "name": "releasenotes",
  "version": "1.0.0",
  "description": "Log Release Notes from Git Repository",
  "main": "getReleaseNotes.js",
  "scripts": {
    "getReleaseNotes": "git fetch --all && node ./ReleaseNotes/getReleaseNotes.js",
    "getJiraReleaseNotes": "git fetch --all && node ./ReleaseNotes/getReleaseNotes.js -- --jira"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleCiotto/ReleaseNotes.git"
  },
