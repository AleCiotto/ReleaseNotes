  "bugs": {
    "url": "https://github.com/AleCiotto/ReleaseNotes/issues"
  },
  "homepage": "https://github.com/AleCiotto/ReleaseNotes#readme",
  "dependencies": {
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  }
}
